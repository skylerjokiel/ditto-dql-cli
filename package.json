{
  "name": "ditto-dql-cli",
  "version": "0.1.4",
  "description": "A sandbox CLI for running DQL queries against Ditto databases with benchmarking and performance tracking",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "dql": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "prepublishOnly": "npm run build",
    "postinstall": "node postinstall.js || true"
  },
  "author": "skylerjokiel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skylerjokiel/ditto-dql-cli.git"
  },
  "keywords": [
    "ditto",
    "dql",
    "database",
    "terminal",
    "cli",
    "benchmark",
    "performance"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/",
    "movies.ndjson",
    "benchmark_baselines.ndjson",
    "scenarios.json",
    "benchmarks.json",
    "postinstall.js",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/skylerjokiel/ditto-dql-cli#readme",
  "bugs": {
    "url": "https://github.com/skylerjokiel/ditto-dql-cli/issues"
  },
  "dependencies": {
    "@dittolive/ditto": "^4.12.2",
    "readline": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
